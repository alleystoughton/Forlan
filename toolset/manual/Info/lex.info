@FILES
  ["lex"; "The Lex Module";]
@SIGNATURE LEX "lex" [
  @TYPES 
    @TYPE sym
    @DATATYPE sym_top [ BasicSymTop CompoundSymTop]
    @DATATYPE tok [ Bar Comma Dollar Perc Plus Semicolon Star Tilde OpenPar ClosPar SingArr DoubArr Sym Heading EOF]
    @TYPE basic
  @VALS 
    symToString
    checkInLabToks
    expectedLetterOrDigit
    lexString
    unexpectedTok
    basicToChar
    expectedLetter
    symTopToSym
    expectedDigit
    lexFile
    error
    equalTok
    sizeSym
    errorNotEOFTerminated
    symToPP
    expectedTok
    charToBasic
    compareSym
    symToSymTop
]
@STRUCTURE Lex "lex" LEX
